<mujoco model="Scanned Objects">
    <!-- Memory? -->
    <size memory="500M"/>
    
    <!-- Default options -->
    <!-- <compiler angle="radian" assetdir="./tabletop/mesh" autolimits="true" /> -->
    <option integrator="RK4" noslip_iterations="20" />
    <default>
        <joint armature="0.1" damping="0.1" />
        <default class="visual_only">
            <geom type="mesh" contype="0" conaffinity="0" group="2" />
        </default>
        <default class="no_visual_collision">
            <!-- visual group of 'collision' should be 3 to not render -->
            <geom type="capsule" solimp=".9 .99 .001" solref=".015 1" group="3" />
        </default>
        <default class="visual_collision">
            <geom type="mesh" solimp=".9 .99 .001" solref=".015 1" group="2" />
        </default>
    </default>
    
    <!-- Assets -->
    <asset>
        <!-- Materials for blocks -->
        <material name="red_block" rgba="0.8 0.2 0.2 1.0" specular="0.5" shininess="0.5"/>
        <material name="blue_block" rgba="0.2 0.2 0.8 1.0" specular="0.5" shininess="0.5"/>
    </asset>
    
    <!-- Sky and floor -->
    <include file="./tabletop/object/floor_isaac_style.xml" />

    <!-- Table -->
    <include file="./tabletop/object/object_table.xml" />
    
    <!-- UR52 -->
   
    <include file="./robotis_omy/omy.xml" />
    
    <!-- Plate -->
    <include file="./objaverse/plate_11/model_new.xml"/>
    
    <!-- Blocks -->
    <worldbody>
        <!-- Red Block -->
        <body name="body_obj_block_red">
            <freejoint />
            <body name="object_block_red">
                <!-- Visual geometry -->
                <geom name="block_red_visual" 
                      type="box" 
                      size="0.025 0.025 0.025" 
                      material="red_block" 
                      contype="0" 
                      conaffinity="0" 
                      group="2" />
                <!-- Collision geometry -->
                <geom name="block_red_collision" 
                      type="box" 
                      size="0.025 0.025 0.025" 
                      rgba="0.8 0.2 0.2 0.0" 
                      solimp="0.998 0.998 0.001" 
                      solref="0.001 1" 
                      density="100" 
                      friction="0.95 0.3 0.1" 
                      group="3" />
            </body>
            <site rgba="0 0 0 0" size="0.005" pos="0 0 -0.025" name="bottom_site_block_red" />
            <site rgba="0 0 0 0" size="0.005" pos="0 0 0.025" name="top_site_block_red" />
            <site rgba="0 0 0 0" size="0.005" pos="0.025 0 0" name="horizontal_radius_site_block_red" />
        </body>
        
        <!-- Blue Block -->
        <body name="body_obj_block_blue">
            <freejoint />
            <body name="object_block_blue">
                <!-- Visual geometry -->
                <geom name="block_blue_visual" 
                      type="box" 
                      size="0.025 0.025 0.025" 
                      material="blue_block" 
                      contype="0" 
                      conaffinity="0" 
                      group="2" />
                <!-- Collision geometry -->
                <geom name="block_blue_collision" 
                      type="box" 
                      size="0.025 0.025 0.025" 
                      rgba="0.2 0.2 0.8 0.0" 
                      solimp="0.998 0.998 0.001" 
                      solref="0.001 1" 
                      density="100" 
                      friction="0.95 0.3 0.1" 
                      group="3" />
            </body>
            <site rgba="0 0 0 0" size="0.005" pos="0 0 -0.025" name="bottom_site_block_blue" />
            <site rgba="0 0 0 0" size="0.005" pos="0 0 0.025" name="top_site_block_blue" />
            <site rgba="0 0 0 0" size="0.005" pos="0.025 0 0" name="horizontal_radius_site_block_blue" />
        </body>
    </worldbody>

</mujoco>